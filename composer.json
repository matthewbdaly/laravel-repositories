{
    "name": "matthewbdaly/laravel-repositories",
    "description": "A base repository class and interface, together with a caching decorator. Extend them for use in your own projects.",
    "type": "library",
    "keywords": ["laravel", "repositories", "decorators", "caching", "cache"],
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0",
        "psy/psysh": "^0.8.14",
        "satooshi/php-coveralls": "^1.0",
        "matthewbdaly/artisan-standalone": "0.0.*",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelRepositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelRepositories\\Providers\\LaravelRepositoriesServiceProvider"
            ]
        }
    }
}
